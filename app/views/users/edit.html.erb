<main class="container">
  <br>
  <div class="row">
    <div class="col-md"></div>
    <div class="col-md-10">
      <h1>Users edit</h1>
      <!-- form_for section -->
      <%= form_with model: current_user do |f| %>
        <div class="form-group">
          <%= f.label :postal_code %><br>
          <%= f.text_field :postal_code %>
        </div>
        <div class="form-group">
          <%= f.label :address %><br>
          <%= f.text_area :address %>
        </div>
        <div class="form-group">
          <%= f.label :phone_number %><br>
          <%= f.text_field :phone_number %>
        </div>
        <div class="form-group">
          <%= f.label :email %><br>
          <%= f.text_field :email %>
        </div>
        <div class="form-group">
          <%= f.label :password %><br>
          <%= f.text_field :password %>
        </div>
        <div class="form-group">
          <%= f.label :image %><br>
          <%= f.text_field :image_id  %>
        </div>
        <div class="form-group">
          <%= f.label :introduction %><br>
          <%= f.text_field :introduction %>
        </div>
      <% end %>
    </div>
    <div class="col-md"></div>
</main>
